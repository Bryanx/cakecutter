<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomView">
        <attr name="viewText" format="string"/>
        <attr name="viewTextSize" format="dimension"/>
        <attr name="viewPadding" format="integer"/>
        <attr name="viewVisible" format="boolean"/>
        <attr name="viewIntArray" format="reference"/>
        <attr name="viewPosition" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>
</resources>